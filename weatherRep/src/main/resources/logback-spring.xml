<configuration>
	
	<appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%black(%d{ISO8601}) %highlight(%-5level) [%blue(%t)] %logger{36} %yellow(%C{}): %msg%n%throwable</pattern>
		</encoder>
	</appender>
	
	<root level="INFO">
		<appender-ref ref="CONSOLE"/>
	</root>
	
	<logger name="com.WReport"/>
	<logger name="com.WReport.config" level="DEBUG"/>

		
</configuration>